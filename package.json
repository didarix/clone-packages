{
  "name": "youxel-frontend-packages",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "/1": "",
    "// version-lib": "to increase patch input in version",
    "version-core": "cd libs/core && npm version patch",
    "version-form": "cd libs/form && npm version patch",
    "version-table": "cd libs/table && npm version patch",
    "version-shared": "cd libs/shared && npm version patch",
    "/2": "",
    "// build-lib": "to increase version then build and output dist directory",
    "build-core": "ng build core",
    "build-form": "ng build  form",
    "build-table": "ng build  table",
    "build-shared": "ng build shared",
    "build-all": "npm run build-core && npm run build-form && npm run build-table  && build-shared && ng build",
    "/3": "",
    "// build-lib-version": "to increase version then build and output dist directory",
    "build-core-version": "npm run version-core && ng build core",
    "build-form-version": "npm run version-form && ng build  form",
    "build-table-version": "npm run version-table && ng build  table",
    "build-shared-version": "npm run version-shared && ng build shared",
    "build-all-version": "npm run build-core && npm run build-form && npm run build-table  && build-shared && ng build",
    "/4": "publish packages locallay",
    "// publish-lib local": "to publish lib to registery",
    "publish-core-local": "cd dist/libs/core &&  npm publish --registry http://localhost:4873/",
    "publish-form-local": "cd dist/libs/form &&  npm publish --registry http://localhost:4873/",
    "publish-table-local": "cd dist/libs/table &&  npm publish --registry http://localhost:4873/",
    "publish-shared-local": "cd dist/libs/shared &&  npm publish --registry http://localhost:4873/",
    "publish-all-local": "npm run publish-core && npm run publish-form && npm run publish-table && publish-shared",
    "/5": "publish packages remotly",
    "// publish-lib-remote": "to publish lib to registery remote",
    "publish-core": "cd dist/libs/core &&  npm publish --registry https://youxelteam.pkgs.visualstudio.com/_packaging/Youxel/npm/registry/",
    "publish-form": "cd dist/libs/form &&  npm publish --registry https://youxelteam.pkgs.visualstudio.com/_packaging/Youxel/npm/registry/",
    "publish-table": "cd dist/libs/table &&  npm publish --registry https://youxelteam.pkgs.visualstudio.com/_packaging/Youxel/npm/registry/",
    "publish-shared": "cd dist/libs/shared &&  npm publish --registry https://youxelteam.pkgs.visualstudio.com/_packaging/Youxel/npm/registry/",
    "publish-all": "npm run publish-core && npm run publish-form && npm run publish-table && publish-shared",
    "/6": "",
    "// unpublish-lib": "to unpublish lib to registery",
    "unpublish-core": "npm unpublish @youxel/core --force",
    "unpublish-form": "npm unpublish @youxel/form --force",
    "unpublish-table": "npm unpublish @youxel/table --force",
    "unpublish-shared": "npm unpublish @youxel/shared --force",
    "unpublish-all": "npm run unpublish-core && npm run unpublish-form && npm run unpublish-table"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "13.1.2",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@azure/msal-angular": "^2.0.1",
    "@azure/msal-browser": "^2.15.0",
    "@ng-select/ng-select": "^5.0.9",
    "@ngx-formly/core": "^5.10.23",
    "@ngx-formly/primeng": "^5.10.23",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "13.4.2",
    "@types/crypto-js": "4.0.2",
    "@types/node-forge": "0.10.10",
    "@types/quill": "^2.0.9",
    "apexcharts": "^3.29.0",
    "chart.js": "^3.6.0",
    "crypto-js": "4.1.1",
    "date-fns": "^2.22.1",
    "moment-hijri": "2.1.2",
    "ng-apexcharts": "^1.5.12",
    "ng-http-loader": "^11.0.0",
    "ngx-material-timepicker": "5.5.3",
    "primeicons": "^5.0.0",
    "primeng": "^12.2.2",
    "quill": "^1.3.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@nrwl/cli": "13.4.2",
    "@nrwl/cypress": "13.4.2",
    "@nrwl/eslint-plugin-nx": "13.4.2",
    "@nrwl/jest": "13.4.2",
    "@nrwl/linter": "13.4.2",
    "@nrwl/tao": "13.4.2",
    "@nrwl/workspace": "13.4.2",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "cypress": "^9.1.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "ng-packagr": "^13.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  }
}
